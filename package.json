{
  "name": "scheduler",
  "version": "1.0.6",
  "description": "",
  "author": "Seweryn",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --path tsconfig.build.json",
    "start": "nest start --path tsconfig.json",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "tsc": "node node_modules/typescript/bin/tsc --project tsconfig.build.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "format": "yarn prettier -w ./src",
    "format:check": "yarn prettier --check ./src",
    "precommit": "yarn tsc && yarn test && yarn code-check",
    "code-check": "yarn prettier -w src && yarn lint --fix && git add ."
  },
  "dependencies": {
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/jwt": "10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "8.4.7",
    "@nestjs/passport": "9.0.1",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/throttler": "3.0.0",
    "@nestjs/typeorm": "9.0.1",
    "body-parser": "1.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "date-fns": "2.29.3",
    "helmet": "6.0.0",
    "ioredis": "5.3.0",
    "mysql": "2.18.1",
    "passport": "^0.6.0",
    "passport-custom": "1.1.1",
    "ramda": "0.28.0",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "typeorm": "0.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nestjs/cli": "9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.2",
    "@types/express": "4.17.14",
    "@types/ioredis": "5.0.0",
    "@types/jest": "29.1.1",
    "@types/node": "18.8.2",
    "@types/ramda": "0.28.15",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-plugin-functional": "4.4.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-nested-if": "1.0.0",
    "eslint-plugin-no-else": "0.2.2",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "supertest": "6.3.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testEnvironment": "node"
  }
}
